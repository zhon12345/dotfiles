{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "lightGreen",
					"leading_diamond": "<foreground>\u256d\u2500</>",
					"template": " \ue795 {{ .Name }}",
					"style": "diamond",
					"type": "shell"
				},
				{
					"foreground": "lightYellow",
					"template": " \ue613 {{ .Path }}",
					"options": {
						"style": "mixed"
					},
					"style": "plain",
					"type": "path"
				},
				{
					"foreground": "lightBlue",
					"template": " {{ .UpstreamIcon }} {{ url .HEAD .UpstreamURL }}{{ if or (or .Ahead .Behind) .Working.Changed .Staging.Changed }} [{{ if .Ahead }}<green> \u2191{{ .Ahead }}</>{{ end }}{{ if .Behind }}<red> \u2193{{ .Behind }}</>{{ end }}{{ if .Working.Changed }} \uea73 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf00c {{ .Staging.String }}{{ end }} ]{{ end }}",
					"options": {
						"fetch_status": true,
						"fetch_upstream_icon": true
					},
					"style": "plain",
					"type": "git"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "right",
			"segments": [
				{
					"foreground": "lightMagenta",
					"template": "\uf252 {{ .FormattedMs }}",
					"options": {
						"always_enabled": true
					},
					"style": "plain",
					"type": "executiontime"
				},
				{
					"foreground_templates": [
						"{{if eq \"Charging\" .State.String}}lightYellow{{end}}",
						"{{if eq \"Discharging\" .State.String}}lightRed{{end}}",
						"{{if eq \"Full\" .State.String}}lightGreen{{end}}"
					],
					"template": " {{ if not .Error }}{{ .Icon }} {{ .Percentage }}%{{ end }}",
					"options": {
						"charged_icon": "\uf1e6",
						"charging_icon": "\uf0e7",
						"discharging_icon": "\uf12a",
						"not_charging_icon": "\udb80\udc79"
					},
					"style": "plain",
					"type": "battery"
				},
				{
					"foreground": "lightCyan",
					"template": " {{ .CurrentDate | date .Format }}",
					"options": {
						"time_format": "15:04:05"
					},
					"style": "plain",
					"type": "time"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "green",
					"leading_diamond": "<foreground>\u2570\u2500</>",
					"template": "<lightGreen>\u276f</><lightYellow>\u276f</><lightBlue>\u276f</>",
					"style": "diamond",
					"type": "text"
				}
			],
			"type": "prompt"
		}
	],
	"transient_prompt": {
		"newline": true,
		"template": "<lightGreen>\u2570\u2500\u276f</><lightYellow>\u276f</><lightBlue>\u276f</> "
	},
	"final_space": true,
	"version": 4
}
